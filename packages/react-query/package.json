{
  "name": "@asouei/safe-fetch-react-query",
  "version": "0.1.0",
  "description": "React Query adapter for @asouei/safe-fetch",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "lint": "echo lint:ok"
  },
  "keywords": [
    "react-query",
    "tanstack",
    "safe-fetch",
    "http",
    "typesafe",
    "fetch",
    "adapter",
    "no-throw"
  ],
  "author": "Aleksandr Mikhailishin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/asouei/safe-fetch.git",
    "directory": "packages/react-query"
  },
  "bugs": {
    "url": "https://github.com/asouei/safe-fetch/issues"
  },
  "homepage": "https://asouei.dev",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "@asouei/safe-fetch": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.9.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "rimraf": "^6.0.1"
  }
}